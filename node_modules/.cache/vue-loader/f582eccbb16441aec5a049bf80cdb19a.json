Item('memo', JSON.stringify(this.memo));\r\n        },\r\n    },\r\n    mounted: function(){\r\n        this.title = JSON.parse(localStorage.getItem('title'))  || [];\r\n        this.date1 = JSON.parse(localStorage.getItem('date1'))  || [];\r\n        this.date2 = JSON.parse(localStorage.getItem('date2'))  || [];\r\n        this.date3 = JSON.parse(localStorage.getItem('date3'))  || [];\r\n        this.memo = JSON.parse(localStorage.getItem('memo'))  || [];\r\n    },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/* ここにcssを書いてね */\r\nheader {\r\n    height: 50px;\r\n    background-color:  #ea5532;\r\n}\r\n\r\nimg {\r\n    height: 40px;\r\n    padding: 5px 0 0 10px;\r\n}\r\n\r\n.title{\r\n    font-size: 20px;\r\n    padding-top: 15px;\r\n    padding-bottom: 15px;\r\n    padding-left: 10px;\r\n    color: rgb(143, 143, 143);\r\n}\r\n\r\n.goalDate, .limitDate, .notification {\r\n    display: flex;\r\n    border-top: 1px solid #ea5532;\r\n    justify-content: space-between;\r\n    color: rgb(143, 143, 143);\r\n}\r\n\r\n.caption{\r\n    padding-left: 10px;\r\n}\r\n\r\n.memo{\r\n    border-top: 1px solid #ea5532;\r\n    padding-top: 16px;\r\n    padding-left: 10px;\r\n    color: rgb(143, 143, 143);\r\n}\r\n\r\n.form {\r\n    border-bottom: 1px solid #ea5532;\r\n    padding-top: 16px;\r\n    padding-bottom: 16px;\r\n    padding-left: 10px;\r\n}\r\n\r\n\r\n.finish {\r\n    background-color: #ea5532;\r\n    width: 150px;\r\n    height: 40px;\r\n    border-radius: 8px;\r\n    line-height: 40px;\r\n    text-align: center;\r\n    margin:0 auto;\r\n    margin-top: 16px;\r\n    box-shadow: 1px 1px #b3381c;\r\n}\r\n\r\n.finish:active {\r\n    box-shadow: none;\r\n    position: relative;\r\n    top: 1px;\r\n    left: 1px;\r\n}\r\n\r\na {\r\n    text-decoration: none;\r\n    color: white;\r\n    display: block;\r\n}\r\n\r\ninput[type=\"datetime-local\"] {\r\n    height: 30px;\r\n    margin-top: 12px;\r\n    margin-right: 10px;\r\n    width: 185px;\r\n}\r\n\r\ninput[type=\"text\"] {\r\n   