"todo.memo\" label=\"メモ\" color=\"#ea5532\"></v-textarea>\r\n    </v-col>\r\n    <div class=\"btn\">\r\n      <DoneButton></DoneButton>\r\n      <SaveButton @click.native=\"SaveData\"></SaveButton>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport DoneButton from '../components/DoneButton.vue'\r\nimport SaveButton from '../components/SaveButton.vue'\r\nimport Header from '../components/Header.vue'\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n    name: 'TodoShowPage',\r\n    components: {\r\n        DoneButton,\r\n        SaveButton,\r\n        Header\r\n    },\r\n    data(){\r\n        return{\r\n            todo: {\r\n              title: '',\r\n              goalDate: '',\r\n              limitDate: '',\r\n              notification: '',\r\n              memo: ''\r\n            },\r\n        }\r\n    },\r\n    // todo一覧からidを取得->そのidに対応するdataをAPIから取得\r\n    mounted() {\r\n      axios\r\n        .get('https://jsonplaceholder.typicode.com/todos/')\r\n        .then(response => {\r\n          this.todo = response.data[this.$route.params.id-1]\r\n          console.log(this.todo)\r\n        })\r\n        .catch(error => {\r\n          console.log(error);\r\n        });\r\n    },\r\n    //データの更新\r\n    methods: {\r\n      SaveData: function(){\r\n      axios\r\n        .put('https://jsonplaceholder.typicode.com/posts/' + this.todo.id, this.todo)\r\n        .then(response => {\r\n          console.log(response.data);\r\n        })\r\n        .catch(error => console.log(error))\r\n      }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.btn {\r\n  text-align: center;\r\n}\r\n\r\n.top {\r\n  margin-top: 16px;\r\n}\r\n\r\n.second {\r\n  margin-top: -32px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/pages"}]}